# PostgreSQL Configuration (Render will provide these)
PG_HOST=your-postgres-host.render.com
PG_USER=postgres
PG_PASSWORD=your-postgres-password
PG_DB=jukejam
PG_PORT=5432

# Redis Configuration (Render will provide these)
REDIS_HOST=your-redis-host.render.com
REDIS_PORT=6379
REDIS_URL=redis://your-redis-host.render.com:6379

# Server Configuration
PORT=4000
NODE_ENV=production

# Frontend URL for CORS (Set this to your Render frontend URL)
FRONTEND_URL=https://jukejam-frontend.onrender.com
