# Example Environment Variables
# Copy this file to .env.development or .env.production and update the values

# GraphQL HTTP endpoint
VITE_GRAPHQL_URL=http://localhost:4000/graphql

# GraphQL WebSocket endpoint (for subscriptions)
VITE_GRAPHQL_WS_URL=ws://localhost:4000/graphql

# Sync WebSocket endpoint (for real-time playback sync)
VITE_SYNC_WS_URL=ws://localhost:3000
